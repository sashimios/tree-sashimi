# Ref: https://aur.archlinux.org/cgit/aur.git/tree/?h=sndcpy-bin&id=37eff7cc67ca4cbee75cdd0682fa1f72a8e9f467
abinfo "Installing sndcpy ..."
install -Dvm755 "$SRCDIR"/sndcpy \
    "$PKGDIR"/usr/lib/sndcpy/sndcpy
install -Dvm644 "$SRCDIR"/sndcpy.apk \
    "$PKGDIR"/usr/lib/sndcpy/sndcpy.apk
